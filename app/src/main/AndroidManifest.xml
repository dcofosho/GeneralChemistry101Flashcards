<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.generalchemistry101flashcards_2">

    <application
    android:allowBackup="true"
    android:icon="@drawable/thermo2"
    android:label="General Chemistry Quizes"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".QuestionActivity"
        android:label="QuestionActivity">
    </activity>
        <activity android:name=".ScoreboardActivity"
            android:label="ScoreboardActivity">
        </activity>
        <activity android:name=".periodicActivity"
            android:label="ScoreboardActivity">
        </activity>
</application>

    </manifest>

